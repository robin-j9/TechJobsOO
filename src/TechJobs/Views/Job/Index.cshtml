@model TechJobs.Models.Job

    <table class="job-listing">
        <tr>
            <th>Name</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>Employer</th>
            <td>@Model.Employer</td>
        </tr>
        <tr>
            <th>Location</th>
            <td>@Model.Location</td>
        </tr>
        <tr>
            <th>Skill</th>
            <td>@Model.CoreCompetency</td>
        </tr>
        <tr>
            <th>Position Type</th>
            <td>@Model.PositionType</td>
        </tr>
    </table>